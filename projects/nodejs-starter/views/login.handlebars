<div class="content_container">
  <div class="page-content" id="learnstuff-login">
    <div class="login-form">
      <form method="POST" id="learnstuff-login-form" name="learnstuff-login" action="/login">
        <div class="form-group">
          <input name="email" id="email" class="form-control input-lg" type="text" placeholder="E-mail">
        </div>
        <div class="form-group">
          <input name="password" id="pass" class="form-control input-lg" type="password" placeholder="Password">
        </div>
        <input type="submit" class="btn button-default btn-lg btn-block login-button" id="login" value="login">
      </form>
    </div>
  </div>
</div>
